{
  "extends": "react-app",
  "rules": {
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/alt-text": "off",
    "react/no-deprecated": "warn",
    "no-useless-constructor": "warn",
    "indent": ["warn", 2],
    "max-len": ["warn", { "code": 100 }],
    "key-spacing": [
      "warn",
      {
        "mode": "strict"
      }
    ],
    "react/jsx-no-duplicate-props": "error",
    "eqeqeq": "error"
  }
}